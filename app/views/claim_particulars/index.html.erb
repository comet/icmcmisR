<h1>Listing claim_particulars</h1>

<table>
  <tr>
    <th>Amount</th>
    <th>Expeccted amount</th>
    <th>Given by</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @claim_particulars.each do |claim_particular| %>
  <tr>
    <td><%= claim_particular.amount %></td>
    <td><%= claim_particular.expeccted_amount %></td>
    <td><%= claim_particular.given_by %></td>
    <td><%= link_to 'Show', claim_particular %></td>
    <td><%= link_to 'Edit', edit_claim_particular_path(claim_particular) %></td>
    <td><%= link_to 'Destroy', claim_particular, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

