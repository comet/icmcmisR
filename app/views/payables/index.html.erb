<h1>Listing payables</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Description</th>
    <th>Type</th>
    <th>Encounter</th>
    <th>Duration</th>
    <th>Price</th>
    <th>Other info</th>
    <th>Category</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @payables.each do |payable| %>
  <tr>
    <td><%= payable.name %></td>
    <td><%= payable.description %></td>
    <td><%= payable.type %></td>
    <td><%= payable.encounter_id %></td>
    <td><%= payable.duration %></td>
    <td><%= payable.price %></td>
    <td><%= payable.other_info %></td>
    <td><%= payable.category %></td>
    <td><%= link_to 'Show', payable %></td>
    <td><%= link_to 'Edit', edit_payable_path(payable) %></td>
    <td><%= link_to 'Destroy', payable, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Payable', new_payable_path %>
